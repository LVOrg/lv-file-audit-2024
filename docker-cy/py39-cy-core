
FROM docker.lacviet.vn/xdoc/py39-fast-client:amd.1 as fast_client
FROM docker.lacviet.vn/xdoc/py39-cy_es:amd.1 as es
FROM docker.lacviet.vn/xdoc/py39-cy_kit:amd.1 as kit
FROM docker.lacviet.vn/xdoc/py39-cy_docs:amd.2 as docs
FROM docker.lacviet.vn/xdoc/py39-cy_web:amd.1 as web
FROM docker.lacviet.vn/xdoc/py39-cy-env-cpu:amd.4 as env
